@model ledsangviet_chuongnh.Models.Account

<!DOCTYPE html>
<html>

<!-- Mirrored from coderthemes.com/uplon_1.4/light/pages-login.html by HTTrack Website Copier/3.x [XR&CO'2014], Sat, 08 Oct 2016 03:26:39 GMT -->
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
    <meta name="author" content="Coderthemes">

    <!-- App Favicon -->
    <link rel="shortcut icon" href="/Assets/Admin/images/favicon.ico">

    <!-- App title -->
    <title>Đăng nhập | Admin</title>

    <!-- App CSS -->
    <link href="/Assets/Admin/css/style.css" rel="stylesheet" type="text/css" />

    <!-- HTML5 Shiv and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->

    <script src="/Assets/Admin/js/modernizr.min.js"></script>

</head>


<body>

    <div class="col-xs-3 col-md-3 col-ld-6">
    </div>
    <div class="col-xs-12 col-md-6 col-ld-3">
        <div class="card-box" style="margin-top: 50px">
            <div class="text-xs m-t-20">
                <a href="#" class="logo">
                    <i class="zmdi zmdi-group-work icon-c-logo" style="padding-left: 18px"></i>
                    <span>LED SÁNG VIỆT</span>
                </a>
            </div>
            <div class="m-t-10 p-20">
                <div class="row">
                    <div class="col-xs-12 text-xs">
                        <h6 class="text-muted text-uppercase m-b-0 m-t-0">Đăng nhập cho phiên làm việc</h6>
                    </div>
                </div>
                <br />
                @using (Html.BeginForm("Login", "Login", FormMethod.Post))
                {
                    <div class="form-group row">
                        <div class="col-xs-12">
                            @Html.TextBoxFor(model => model.Username, new { @class = "form-control", @type = "text", required = "", @placeholder = "Username" })
                            @Html.ValidationMessageFor(model => model.Username, "", new { @class = "text-danger" })
                        </div>
                    </div>

                    <div class="form-group row">
                        <div class="col-xs-12">
                            @Html.TextBoxFor(model => model.Password, new { @class = "form-control", @type = "password", required = "", @placeholder = "Password" })
                            @Html.ValidationMessageFor(model => model.Password, "", new { @class = "text-danger" })
                        </div>
                    </div>
                    <div class="form-group text-center row m-t-10">
                        <div class="col-xs-12">
                            <button class="btn btn-success btn-block waves-effect waves-light" type="submit">Đăng nhập</button>
                        </div>
                    </div>
                }
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
    <!-- end card-box-->

    <script>
        var resizefunc = [];
    </script>

    <!-- jQuery  -->
    <script src="/Assets/Admin/js/jquery.min.js"></script>
    <script src="/Assets/Admin/js/tether.min.js"></script><!-- Tether for Bootstrap -->
    <script src="/Assets/Admin/js/bootstrap.min.js"></script>
    <script src="/Assets/Admin/js/detect.js"></script>
    <script src="/Assets/Admin/js/fastclick.js"></script>
    <script src="/Assets/Admin/js/jquery.blockUI.js"></script>
    <script src="/Assets/Admin/js/waves.js"></script>
    <script src="/Assets/Admin/js/jquery.nicescroll.js"></script>
    <script src="/Assets/Admin/js/jquery.scrollTo.min.js"></script>
    <script src="/Assets/Admin/js/jquery.slimscroll.js"></script>
    <script src="/Assets/Admin/plugins/switchery/switchery.min.js"></script>

    <!-- App js -->
    <script src="/Assets/Admin/js/jquery.core.js"></script>
    <script src="/Assets/Admin/js/jquery.app.js"></script>

</body>

<!-- Mirrored from coderthemes.com/uplon_1.4/light/pages-login.html by HTTrack Website Copier/3.x [XR&CO'2014], Sat, 08 Oct 2016 03:26:39 GMT -->
</html>