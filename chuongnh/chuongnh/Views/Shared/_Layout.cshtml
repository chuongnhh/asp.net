
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />

    <meta name="description" content="Computer Center Activities" />
    <meta name="author" content="Nguyen Hoang Chuong" />

    <title>@ViewBag.Title</title>
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&amp;subset=all" rel="stylesheet" type="text/css" />
    <link href="/Assets/Frontend/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="/Assets/Frontend/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link href="/Assets/Frontend/global/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="/Assets/Frontend/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN THEME GLOBAL STYLES -->
    <link href="/Assets/Frontend/global/css/components.min.css" rel="stylesheet" id="style_components" type="text/css" />
    <link href="/Assets/Frontend/global/css/plugins.min.css" rel="stylesheet" type="text/css" />
    <!-- END THEME GLOBAL STYLES -->
    <link href="~/Assets/Frontend/layout/css/site.css" rel="stylesheet" />
    @RenderSection("cssHeader", required: false)
    <!-- BEGIN THEME LAYOUT STYLES -->
    <link href="/Assets/Frontend/layout/css/layout.min.css" rel="stylesheet" type="text/css" />
    <link href="/Assets/Frontend/layout/css/themes/default.min.css" rel="stylesheet" type="text/css" id="style_color" />
    <link href="/Assets/Frontend/layout/css/custom.min.css" rel="stylesheet" type="text/css" />
    <!-- END THEME LAYOUT STYLES -->
    <link rel="shortcut icon" href="/Assets/favicon.ico" />
</head>
<body class="page-container-bg-solid page-header-menu-fixed">
    <div class="page-wrapper">
        @RenderPage("Header.cshtml")
        @RenderBody()
        @RenderPage("Footer.cshtml")
    </div>
    <!-- BEGIN CORE PLUGINS -->
    <script src="/Assets/Frontend/global/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="/Assets/Frontend/global/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/Assets/Frontend/global/plugins/js.cookie.min.js" type="text/javascript"></script>
    <script src="/Assets/Frontend/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="/Assets/Frontend/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="/Assets/Frontend/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <!-- BEGIN THEME GLOBAL SCRIPTS -->
    <script src="/Assets/Frontend/global/scripts/app.min.js" type="text/javascript"></script>
    <!-- END THEME GLOBAL SCRIPTS -->
    @RenderSection("jsFooter", required: false)
    <!-- BEGIN THEME LAYOUT SCRIPTS -->
    <script src="/Assets/Frontend/layout/scripts/layout.min.js" type="text/javascript"></script>
    <script src="/Assets/Frontend/layout/scripts/demo.min.js" type="text/javascript"></script>
    <script src="/Assets/Frontend/global/scripts/quick-sidebar.min.js" type="text/javascript"></script>
    <script src="/Assets/Frontend/global/scripts/quick-nav.min.js" type="text/javascript"></script>
    <!-- END THEME LAYOUT SCRIPTS -->
    <!-- End -->
</body>
</html>
